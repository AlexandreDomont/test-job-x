pipeline {
	agent any
 	environment {
        	GO111MODULE = 'auto'
    	}	
	tools {
		go 'go-1.20.1'
	}
	stages {
		stage('Build') {
			steps { sh 'go run math/math.go'}
			}
		
		stage ('Tests') {
			steps {sh 'go test -v math/'}
		}
		stage ('Test Coverage') {
                        steps {sh 'go test -coverprofile=coverage.out math/' sh 'go tool cover -html=coverage.out math/'}
                }
		

	}
}

